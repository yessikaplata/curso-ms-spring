package co.com.pragma.customer.servicecustomer.error;

import org.springframework.http.HttpStatus;

public class ServiceCustomerException extends Exception {

	/**
	 * 
	 */
	private static final long serialVersionUID = 7948328251075120684L;

	private HttpStatus httpStatus;
	
	public ServiceCustomerException(String errorMessage) {
		super(errorMessage);
	}

	public ServiceCustomerException(String errorMessage, Throwable err) {
		super(errorMessage, err);
	}
}
